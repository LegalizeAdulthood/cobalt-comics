{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
  "name": "comics-cobalt",
  "version": "1.0.0",
  "dependencies": [
    "boost-cobalt",
    "gtest",
    "simdjson"
  ],
  "default-features": [ "cobalt-examples" ],
  "features": {
    "cobalt-examples": {
      "description": "Dependencies needed only by the boost.cobalt examples",
      "dependencies": [
        "boost-beast",
        "boost-callable-traits",
        "boost-json",
        "boost-lockfree",
        "boost-outcome",
        "boost-signals2",
        "boost-url",
        "openssl"
      ]
    }
  }
}
